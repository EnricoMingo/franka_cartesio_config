solver_options:
    regularization: 1e-4
    back_end: "qpoases"

stack:
    - ["ArmEE"]
    - ["Postural"]

    
constraints: ["JointLimits", "VelocityLimits"]
    

ArmEE:
    type: "Cartesian"
    distal_link: "panda_link8"
    base_link: "panda_link0"
    lambda: 0.1
    indices: [0,1,2,3,4,5]
    
ArmVS:
    lib_name: "libCartesioVisualServoing.so"
    type: "VisualServoing"
    distal_link: "METTERE_LINK_CORRETTO"
    base_link: "panda_link0"
    lambda: 0.001
    feature_type: "vpFeaturePoint"
    number_of_features: 4

Postural:
    type: "Postural"
    lambda: 0.01
    
JointLimits:
    type: "JointLimits"


VelocityLimits:
    type: "VelocityLimits"
